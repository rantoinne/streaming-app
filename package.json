{
  "name": "rantoinne-streaming",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "node prod",
    "dev": "concurrently -n \"JS,SW,TS,ES,FE\" -c \"yellow,cyan,magenta,green\" \"npm run back:watch:server\" \"npm run back:watch:typescript\" \"npm run back:watch:typescript:tsc\" \"npm run back:lint:watch\" \"npm run front:dev\"",
    "back:dev": "concurrently -n \"JS,TS,ES\" -c \"yellow,cyan,magenta\" \"npm run back:watch:server\" \"npm run back:watch:typescript\" \"npm run back:lint:watch\"",
    "back:build": "rimraf prod && swc server --config-file server/.swcrc --out-dir prod --copy-files --source-root server --strip-leading-paths",
    "back:watch:server": "sleep 1 && export TZ=UTC && nodemon prod --watch prod --watch .env --delay 50ms",
    "back:watch:typescript": "swc server --config-file server/.swcrc --out-dir prod --copy-files --source-root server --strip-leading-paths --watch",
    "back:watch:typescript:tsc": "tsc --skipLibCheck -p server --preserveWatchOutput --watch --noEmit",
    "back:lint:watch": "nodemon --exec \"yarn back:lint\" --watch server -e js,ts",
    "back:lint": "eslint server -c server/eslint.config.js",
    "db:migrate": "node_modules/.bin/sequelize db:migrate",
    "db:migrate:undo": "node_modules/.bin/sequelize db:migrate:undo",
    "db:migrate:reset": "node_modules/.bin/sequelize db:migrate:undo:all",
    "db:migration": "node_modules/.bin/sequelize migration:generate --name",
    "front:dev": "vite serve ./client",
    "front:build": "vite build ./client",
    "front:preview": "vite preview ./client",
    "front:lint": "eslint client --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "prepare": "husky",
    "format": "prettier --write ./client && prettier --write ./server",
    "commit": "git add . && cz",
    "c": "yarn commit"
  },
  "dependencies": {
    "@calcom/embed-react": "^1.5.2",
    "@emotion/react": "^11.13.3",
    "@faker-js/faker": "^9.2.0",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@tanstack/react-query": "^5.59.19",
    "@tanstack/react-query-devtools": "^5.62.10",
    "@types/express-session": "^1.18.0",
    "@types/react-gtm-module": "^2.0.4",
    "ajv": "^8.17.1",
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.19.0",
    "bullmq": "^5.40.0",
    "commitizen": "^4.3.1",
    "concat-stream": "^2.0.0",
    "connect-redis": "^7.1.1",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "express-validator": "^7.1.0",
    "fluent-ffmpeg": "^2.1.3",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "nanoid": "^5.0.9",
    "pg": "^8.11.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-gtm-module": "^2.0.11",
    "react-icons": "^5.4.0",
    "react-markdown": "^10.0.0",
    "react-router-dom": "^6.26.2",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.2.1",
    "rimraf": "^5.0.7",
    "sequelize": "^6.37.6",
    "sequelize-cli": "^6.6.2",
    "slugify": "^1.6.6",
    "source-map-support": "^0.5.21",
    "typescript": "^5.7.3",
    "uuid": "^9.0.1",
    "vite-tsconfig-paths": "^5.1.4",
    "winston": "^3.11.0",
    "ws": "^8.17.1",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.12",
    "@types/bluebird": "^3.5.30",
    "@types/body-parser": "^1.17.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.17.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.14.7",
    "@types/pg": "^8.11.2",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/sequelize": "^4.28.20",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.18.0",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "@vitejs/plugin-react": "^4.3.1",
    "chokidar": "^4.0.3",
    "concurrently": "^9.1.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.9.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-react": "^7.35.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.1",
    "nodemon": "^3.1.4",
    "prettier": "3.4.2",
    "ts-node-dev": "^2.0.0",
    "typescript-eslint": "^8.4.0",
    "vite": "^5.4.3"
  },
  "overrides": {
    "node-fetch@2.x": {
      "whatwg-url": "14.x"
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && npx cz --hook || true"
    }
  }
}
